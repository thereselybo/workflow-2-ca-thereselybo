{
  "compilerOptions": {
    "target": "es6",
    "module": "es2015",
    "outDir": "./dist/",
    "rootDir": "./src",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["google.maps", "jest"],
    "moduleResolution": "node"
  },
  "include": ["src"]
}
